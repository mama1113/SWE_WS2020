{
  "name": "beispiel",
  "version": "1.0.0",
  "description": "Beispiel",
  "main": "dist/index.js",
  "engines": {
    "node": ">=14.13.0",
    "npm": ">=6.14.8"
  },
  "type": "commonjs",
  "scripts": {
    "start": "node dist/index.js",
    "start:dev": "npm run init-dist && nodemon --exec ts-node src/index.ts --unhandled-rejections=strict",
    "start:debug": "npm run prepare && nodemon --inspect dist/index.js ",
    "pretest": "npm run init-dist",
    "test": "jest --detectOpenHandles --forceExit",
    "test:coverage": "npm t -- --coverage",
    "app-metrics": "echo Umgebungsavariable setzen:   NODE_OPTIONS=\"--require appmetrics-dash/attach\"",
    "clinic": "clinic doctor -- node dist/index.js",
    "asciidoc": "ts-node scripts/asciidoc",
    "clean": "ts-node scripts/clean",
    "curl:heroku": "curl --include --tlsv1.2 --insecure https://gener-iert-12345.herokuapp.com/buecher/00000000-0000-0000-0000-000000000001",
    "curl:openshift": "curl --include http://beispiel-jz.apps.us-east-1.starter.openshift-online.com:3000/buecher/00000000-0000-0000-0000-000000000001",
    "depcheck": "depcheck",
    "deploy:heroku": "git push heroku master",
    "deploy:openshift": "npm run deploy:prepare && npx nodeshift --expose --deploy.port 3000",
    "deploy:prepare": "concurrently --prefix \"[{name}]\" --names \"ESLint,ejs-lint,init-dist,TypeScript\" --prefix-colors \"yellow.bold,gray.bold,cyan.bold,green.bold\" \"npm run eslint\" \"npm run ejs-lint\" \"npm run init-dist\" \"npm run tsc\"",
    "predocker": "npm run tsc",
    "docker": "ts-node scripts/docker",
    "ejs-lint": "node ./node_modules/ejs-lint/index.js",
    "eslint": "eslint src/**/*.ts",
    "init-dist": "ts-node scripts/init-dist",
    "jenkins": "ts-node scripts/jenkins",
    "leasot": "leasot --exit-nicely src/**/*.ts",
    "mailserver": "ts-node scripts/mailserver",
    "mongodb": "ts-node scripts/mongodb",
    "open:heroku": "heroku open --app gener-iert-12345 /api/buecher/00000000-0000-0000-0000-000000000001",
    "open:heroku:file": "heroku open --app gener-iert-12345 /api/buecher/00000000-0000-0000-0000-000000000001/file",
    "open:heroku:html": "heroku open --app gener-iert-12345 /html/suche",
    "open:heroku:playground": "heroku open --app gener-iert-12345 /api",
    "prettier": "prettier --write src/**/*.ts scripts/*.?s test/**/*.ts",
    "revealjs": "asciidoctor-revealjs doc/folien/folien.adoc",
    "sonarqube": "ts-node scripts/sonarqube",
    "tsc": "npx tsc",
    "heroku-postbuild": "npm run tsc --version && npm run tsc && node scripts/init-heroku"
  },
  "files": [
    "dist",
    "package.json",
    ".env",
    ".gitignore",
    ".npmrc",
    "tsconfig.json",
    "scripts"
  ],
  "private": true,
  "homepage": "https://www.hs-karlsruhe.de",
  "keywords": [
    "Node",
    "npm",
    "Express",
    "body-parser",
    "validator",
    "TypeScript",
    "ESLint",
    "REST",
    "GraphQL",
    "Apollo",
    "MongoDB",
    "mongoose",
    "JWT",
    "bcrypt",
    "SHA-3",
    "CSP",
    "Clickjacking",
    "CORS",
    "nodemailer",
    "winston",
    "EJS",
    "Bootstrap",
    "Jest",
    "ts-jest",
    "fetch",
    "Chai",
    "ts-node",
    "nodemon"
  ],
  "author": "Juergen Zimmermann",
  "license": "GPL-3.0",
